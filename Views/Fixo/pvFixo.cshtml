@model List<RendaFixaViewModel>

<table class="table">
    <thead>
        <tr>
            <th class= "white-text" scope="col">Descrição</th>
            <th class= "white-text" scope="col">Duração</th>
            <th class= "white-text" scope="col">Rentabilidade</th>
            <th class= "white-text" scope="col"></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var renda in Model)
        {
            <tr>
                <td class= "white-text">@renda.Descricao</td>
                <td class= "white-text">@renda.Duracao meses</td=>
                <td class= "white-text">@renda.Rentabilidade %</td>
                <td>
                    <a onclick="confirmarAplicacao(@renda.Id)" class="btn btn-success">Aplicar</a>
                </td>
            </tr>
        }
    </tbody>
</table>
